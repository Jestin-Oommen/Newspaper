{
  "compilerOptions": {
     "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    },
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "components/NewsForm.jsx", "prisma/seed.js", "components/PoliticsNews.tsx", "app/api/articles/[id]/route.js", "app/api/articles/route.js", "app/manage/page.jsx", "app/dashboard/news/page.jsx", "app/search/page.jsx", "lib/auth.js", "app/api/auth/[...nextauth]/route.js", "app/api/uploadthing/core.ts", "app/dashboard/enewspaper/page.jsx", "uploadthing.config.js"  ],
  "exclude": [
    "node_modules"
  ]
}
